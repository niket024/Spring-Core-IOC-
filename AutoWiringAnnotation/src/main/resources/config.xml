<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
	<bean id="address1" class="com.nik.Address" scope="prototype">
		<property name="city" value="banglore"></property>
		<property name="state" value="kar"></property>
	</bean>
	
	
	<bean id="p1" class="com.nik.Person">
		<property name="id" value="123"></property>
		<property name="fname" value="tttt"></property>
		<property name="lname" value="kkk"></property>
	</bean>
	
	<context:annotation-config />

</beans>